// Render new objects to a HTML string
renderedObjects = "<%=j render(objects) %>";

// Append to existing list
appendedDOM = $("#js-infinite-scrolling").append(renderedObjects);

// Format times for new DOM elements
new TimeFormatter(appendedDOM);

// Update Paginator
$('#js-paging').replaceWith("<%=j render('kaminari/paging', objects: objects) %>");
